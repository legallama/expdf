{
  "routes": [
    {
      "route": "/taskpane.html",
      "serve": "/taskpane.html"
    },
    {
      "route": "/functions.html",
      "serve": "/functions.html"
    },
    {
      "route": "/manifest.xml",
      "serve": "/manifest.xml"
    },
    {
      "route": "/*",
      "serve": "/taskpane.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/taskpane.html",
      "statusCode": 200
    }
  }
}